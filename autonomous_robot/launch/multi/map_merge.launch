<launch>
  <arg name="robot_name" default="error"/>
  <group ns="map_merge">
    <node pkg="multirobot_map_merge" type="map_merge" respawn="false" name="map_merge" output="screen">
      <param name="robot_map_topic" value="map"/>
      <param name="robot_namespace" value=""/>
      <param name="merged_map_topic" value="merged"/>
      <param name="world_frame" value="world"/>
      <param name="known_init_poses" value="false"/>
      <param name="merging_rate" value="0.1"/>
      <param name="discovery_rate" value="0.05"/>
      <param name="estimation_rate" value="0.5"/>
      <param name="robot_name" value="$(arg robot_name)"/>
      <param name="estimation_confidence" value="0.4"/>
    </node>
  </group>
</launch>
